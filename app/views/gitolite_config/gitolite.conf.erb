@admins = <%= admin_usernames.join(' ') %>

repo gitolite-admin
    RW+     =   @admins

repo [a-zA-Z0-9].*
    RW+     =   @admins
    C       =   @admins

<% sources.each do |source| %>
repo <%= source.path %>
    RW+     =   @admins
<% end %>
<% usernames.each do |username| %>
repo <%= username %>/.*
    RW master = <%= username %>
<% end %>
